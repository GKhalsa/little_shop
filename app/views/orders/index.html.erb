<h4 class="order-for">Your Orders</h4>

<table class="highlight">
    <tr>
      <th>Order</th>
      <th>Items</th>
      <th>Total</th>
    </tr>
    <% @orders.each do |order|%>
    <tr>
      <td><%= link_to order.id, invoices_path(order_id: order.id) %></td>
      <td><%= order.quantity %></td>
      <td><%= order.total %></td>
    </tr>
  <% end %>
</table>
